{
  "name": "@willsoto/nestjs-objection",
  "description": "Objection module for NestJS",
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "version": "2.0.0",
  "main": "dist/nestjs-objection.umd.js",
  "module": "dist/nestjs-objection.esm.js",
  "files": [
    "dist",
    "lib",
    "typings"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "rollup --config rollup.config.js",
    "postbuild": "yarn typings",
    "clean": "rimraf dist",
    "lint": "eslint lib --ext ts --fix",
    "prerelease": "yarn build",
    "release": "standard-version --sign --message 'chore(release): %s [skip ci]'",
    "postrelease": "conventional-github-releaser -p angular",
    "test": "jest",
    "pretypings": "rimraf typings",
    "typings": "tsc --project tsconfig.build.json --declaration --declarationDir typings --emitDeclarationOnly"
  },
  "typings": "./typings/index.d.ts",
  "peerDependencies": {
    "@nestjs/common": ">=6.2.0",
    "@nestjs/core": ">=6.2.0",
    "knex": ">=0.16.5",
    "objection": ">=1.6.8"
  },
  "devDependencies": {
    "@babel/core": "~7.6.4",
    "@babel/plugin-proposal-decorators": "~7.6.0",
    "@babel/preset-env": "~7.6.3",
    "@babel/preset-typescript": "~7.6.0",
    "@commitlint/cli": "~8.2.0",
    "@commitlint/config-conventional": "~8.2.0",
    "@godaddy/terminus": "~4.2.0",
    "@nestjs/common": "~6.9.0",
    "@nestjs/core": "~6.9.0",
    "@nestjs/terminus": "~6.5.2",
    "@nestjs/testing": "~6.9.0",
    "@types/jest": "~24.0.18",
    "@typescript-eslint/eslint-plugin": "~2.6.0",
    "@typescript-eslint/parser": "~2.6.0",
    "@willsoto/eslint-config-base": "~2.0.0",
    "babel-jest": "~24.9.0",
    "conventional-github-releaser": "~3.1.3",
    "eslint": "~6.6.0",
    "eslint-config-prettier": "~6.5.0",
    "husky": "~3.0.8",
    "jest": "~24.9.0",
    "knex": "~0.20.0",
    "lint-staged": "~9.4.2",
    "objection": "~1.6.9",
    "prettier": "~1.18.2",
    "prettier-package-json": "~2.1.0",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~3.0.0",
    "rollup": "~1.26.0",
    "rollup-plugin-eslint": "~7.0.0",
    "rollup-plugin-filesize": "~6.2.0",
    "rollup-plugin-typescript": "~1.0.1",
    "sqlite3": "~4.1.0",
    "standard-version": "~7.0.0",
    "typescript": "~3.6.4"
  },
  "keywords": [
    "database",
    "knex",
    "nestjs",
    "objection"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{md,yaml,yml}": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "yarn lint",
      "prettier --write",
      "git add"
    ],
    "package.json": [
      "prettier-package-json --write",
      "git add"
    ]
  },
  "volta": {
    "node": "12.12.0",
    "yarn": "1.19.1"
  }
}
