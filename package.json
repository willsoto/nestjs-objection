{
  "author": {
    "email": "willsoto@users.noreply.github.com",
    "name": "Will Soto",
    "url": "https://github.com/willsoto"
  },
  "description": "Objection module for NestJS",
  "devDependencies": {
    "@babel/core": "~7.4.4",
    "@babel/plugin-proposal-decorators": "~7.4.4",
    "@babel/preset-env": "~7.4.4",
    "@babel/preset-typescript": "~7.3.3",
    "@commitlint/cli": "~7.6.1",
    "@commitlint/config-conventional": "~7.6.0",
    "@nestjs/common": "~6.1.0",
    "@nestjs/core": "~6.1.0",
    "@nestjs/testing": "~6.2.0",
    "@types/jest": "~24.0.12",
    "@typescript-eslint/eslint-plugin": "~1.8.0",
    "@willsoto/eslint-config-base": "~2.0.0",
    "babel-jest": "~24.8.0",
    "conventional-github-releaser": "~3.1.2",
    "eslint": "~5.16.0",
    "eslint-config-prettier": "~4.2.0",
    "husky": "~2.3.0",
    "jest": "~24.8.0",
    "knex": "~0.16.3",
    "lint-staged": "~8.1.6",
    "objection": "~1.6.3",
    "prettier": "~1.17.0",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~2.6.3",
    "rollup": "~1.11.3",
    "rollup-plugin-eslint": "~5.1.0",
    "rollup-plugin-filesize": "~6.0.1",
    "rollup-plugin-typescript": "~1.0.1",
    "sqlite3": "~4.0.8",
    "standard-version": "~6.0.1",
    "typescript": "~3.4.5"
  },
  "files": [
    "lib",
    "dist",
    "typings"
  ],
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  },
  "keywords": [
    "nestjs",
    "objection",
    "knex",
    "database"
  ],
  "license": "Apache-2.0",
  "lint-staged": {
    "*.md": [
      "prettier --write",
      "git add"
    ],
    "*.ts": [
      "yarn lint",
      "prettier --write",
      "git add"
    ]
  },
  "main": "dist/nestjs-objection.umd.js",
  "module": "dist/nestjs-objection.esm.js",
  "name": "@willsoto/nestjs-objection",
  "peerDependencies": {
    "@nestjs/common": ">=6.2.0",
    "@nestjs/core": ">=6.2.0",
    "knex": ">=0.16.5",
    "objection": ">=1.6.8"
  },
  "scripts": {
    "build": "rollup --config rollup.config.js",
    "clean": "rimraf dist",
    "lint": "eslint lib --ext ts --fix",
    "postbuild": "yarn typings",
    "postrelease": "conventional-github-releaser -p angular",
    "prebuild": "yarn clean",
    "prerelease": "yarn build",
    "pretypings": "rimraf typings",
    "release": "standard-version --sign --message 'chore(release): %s [skip ci]'",
    "test": "jest",
    "typings": "tsc --project tsconfig.build.json --declaration --declarationDir typings --emitDeclarationOnly"
  },
  "typings": "./typings/index.d.ts",
  "version": "1.1.0"
}
