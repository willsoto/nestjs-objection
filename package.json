{
  "name": "@willsoto/nestjs-objection",
  "version": "2.2.0",
  "description": "Objection module for NestJS",
  "keywords": [
    "database",
    "knex",
    "nestjs",
    "objection"
  ],
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "main": "dist/nestjs-objection.umd.js",
  "module": "dist/nestjs-objection.esm.js",
  "typings": "./typings/index.d.ts",
  "files": [
    "dist",
    "lib",
    "typings"
  ],
  "scripts": {
    "prebuild": "yarn clean",
    "build": "rollup --config rollup.config.js",
    "postbuild": "yarn typings",
    "clean": "rimraf dist",
    "lint": "eslint lib --ext ts --fix",
    "prerelease": "yarn build",
    "release": "standard-version --sign",
    "postrelease": "conventional-github-releaser -p angular",
    "test": "jest",
    "pretypings": "rimraf typings",
    "typings": "tsc --project tsconfig.build.json --declaration --declarationDir typings --emitDeclarationOnly"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": [
      "yarn lint",
      "prettier --write"
    ],
    "*.{md,yaml,yml}": [
      "prettier --write"
    ],
    "README.md": [
      "markdown-toc -i",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "9.0.1",
    "@commitlint/config-conventional": "9.0.1",
    "@godaddy/terminus": "4.4.1",
    "@nestjs/common": "6.11.11",
    "@nestjs/core": "6.11.11",
    "@nestjs/terminus": "6.5.6",
    "@nestjs/testing": "7.0.0",
    "@types/jest": "~26.0.0",
    "@typescript-eslint/eslint-plugin": "2.34.0",
    "@typescript-eslint/parser": "2.34.0",
    "@willsoto/eslint-config-base": "2.0.0",
    "conventional-github-releaser": "3.1.3",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.11.0",
    "husky": "4.2.5",
    "jest": "26.0.1",
    "knex": "0.21.1",
    "lint-staged": "10.2.11",
    "markdown-toc": "~1.2.0",
    "objection": "2.2.0",
    "prettier": "2.0.5",
    "prettier-plugin-organize-imports": "~1.1.0",
    "prettier-plugin-packagejson": "~2.2.3",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rollup": "2.18.0",
    "rollup-plugin-eslint": "7.0.0",
    "rollup-plugin-filesize": "9.0.1",
    "rollup-plugin-typescript": "1.0.1",
    "rxjs": "6.5.5",
    "sqlite3": "4.2.0",
    "standard-version": "8.0.0",
    "ts-jest": "~26.1.0",
    "typescript": "3.9.5"
  },
  "peerDependencies": {
    "@nestjs/common": ">=6.2.0",
    "@nestjs/core": ">=6.2.0",
    "knex": ">=0.16.5",
    "objection": ">=1.6.8"
  },
  "volta": {
    "node": "12.12.0",
    "yarn": "1.22.4"
  }
}
