{
  "name": "@willsoto/nestjs-objection",
  "version": "5.1.0",
  "description": "Objection module for NestJS",
  "keywords": [
    "database",
    "knex",
    "nestjs",
    "objection"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/willsoto/nestjs-objection.git"
  },
  "license": "Apache-2.0",
  "author": "Will Soto <willsoto@users.noreply.github.com> (https://github.com/willsoto)",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "prebuild": "npm run clean",
    "build": "tsc --project tsconfig.build.json",
    "clean": "rimraf dist coverage .nyc_output",
    "lint": "eslint . --fix",
    "prerelease": "npm run build",
    "release": "semantic-release",
    "test": "mocha",
    "pretest:cov": "rimraf coverage .nyc_output",
    "test:cov": "nyc mocha",
    "test:watch": "mocha --watch",
    "prepare": "husky install"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "*.ts": [
      "npm run lint",
      "prettier --write"
    ],
    "*.{md,yaml,yml}": [
      "prettier --write"
    ],
    "README.md": [
      "markdown-toc -i",
      "prettier --write"
    ]
  },
  "prettier": {
    "arrowParens": "always",
    "semi": true,
    "singleQuote": false,
    "trailingComma": "all"
  },
  "mocha": {
    "extension": [
      "ts"
    ],
    "file": "./ad-hocs/mocha/plugins.ts",
    "recursive": true,
    "require": [
      "ts-node/register",
      "source-map-support/register",
      "./ad-hocs/mocha/hooks.ts"
    ],
    "sort": true,
    "watch-files": [
      "src/**/*.ts",
      "test/**/*.ts"
    ]
  },
  "nyc": {
    "all": true,
    "branches": 90,
    "check-coverage": true,
    "extends": "@istanbuljs/nyc-config-typescript",
    "functions": 90,
    "include": [
      "src/**/*.ts"
    ],
    "lines": 90,
    "reporter": [
      "text-summary",
      "html"
    ],
    "statements": 90
  },
  "devDependencies": {
    "@commitlint/cli": "~13.2.1",
    "@commitlint/config-conventional": "~13.2.0",
    "@istanbuljs/nyc-config-typescript": "~1.0.1",
    "@nestjs/common": "~8.1.1",
    "@nestjs/core": "~8.1.1",
    "@nestjs/testing": "~8.1.0",
    "@semantic-release/changelog": "~6.0.0",
    "@types/chai": "~4.2.22",
    "@types/chai-as-promised": "~7.1.4",
    "@types/mocha": "~9.0.0",
    "@types/node": "~16.11.0",
    "@types/sinon": "~10.0.4",
    "@types/sinon-chai": "~3.2.5",
    "@typescript-eslint/eslint-plugin": "~4.33.0",
    "@typescript-eslint/parser": "~4.33.0",
    "chai": "~4.3.4",
    "chai-as-promised": "~7.1.1",
    "eslint": "~7.32.0",
    "eslint-config-prettier": "~8.3.0",
    "eslint-plugin-mocha": "~9.0.0",
    "husky": "~7.0.2",
    "knex": "~0.95.11",
    "lint-staged": "~11.2.2",
    "markdown-toc": "~1.2.0",
    "mocha": "~9.1.2",
    "nyc": "~15.1.0",
    "objection": "3.0.0-alpha.5",
    "prettier": "~2.4.1",
    "prettier-plugin-organize-imports": "~2.3.4",
    "prettier-plugin-packagejson": "~2.2.13",
    "reflect-metadata": "~0.1.13",
    "rimraf": "~3.0.2",
    "rxjs": "~7.4.0",
    "semantic-release": "~18.0.0",
    "sinon": "~11.1.2",
    "sinon-chai": "~3.7.0",
    "source-map-support": "~0.5.20",
    "sqlite3": "~5.0.2",
    "ts-node": "~10.3.0",
    "typescript": "~4.4.3"
  },
  "peerDependencies": {
    "@nestjs/common": "^7.0.0 || ^8.0.0",
    "@nestjs/core": "^7.0.0 || ^8.0.0",
    "knex": "^0.95.0",
    "objection": "^3.0.0"
  },
  "release": {
    "branches": [
      "main",
      {
        "name": "next",
        "prerelease": true
      }
    ],
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github",
      "@semantic-release/npm",
      "@semantic-release/changelog"
    ]
  },
  "volta": {
    "node": "14.17.0",
    "npm": "7.6.0"
  }
}
